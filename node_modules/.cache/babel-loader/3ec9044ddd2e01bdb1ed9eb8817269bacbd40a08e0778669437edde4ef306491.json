InstancesView\",\n  data() {\n    return {\n      courses: [],\n      newInstance: {\n        course: \"\",\n        year: \"\",\n        semester: \"\",\n      },\n    };\n  },\n  methods: {\n    getCourses() {\n      axios\n        .get(\"http://127.0.0.1:8000/api/courses/\")\n        .then((response) => {\n          this.courses.push(...response.data);\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    clearText() {\n      this.newInstance.course = \"\";\n      this.newInstance.year = \"\";\n      this.newInstance.semester = \"\";\n    },\n    createNewInstance() {\n      axios\n        .get(\"http://127.0.0.1:8000/api/courses/\", this.newInstance)\n        .then((response) => {\n          this.courses.push(...response.data);\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n  },\n  mounted() {\n    this.getCourses();\n  },\n};\n</script>\n"],"mappings":";AAkFA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,WAAW,EAAE;QACXC,MAAM,EAAE,EAAE;QACVC,IAAI,EAAE,EAAE;QACRC,QAAQ,EAAE;MACZ;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACXT,KAAI,CACDU,GAAG,CAAC,oCAAoC,EACxCC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACT,OAAO,CAACU,IAAI,CAAC,GAAGD,QAAQ,CAACV,IAAI,CAAC;MACrC,CAAC,EACAY,KAAK,CAAEC,KAAK,IAAK;QAChBC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC,CAAC;IACN,CAAC;IACDG,SAASA,CAAA,EAAG;MACV,IAAI,CAACd,WAAW,CAACC,MAAK,GAAI,EAAE;MAC5B,IAAI,CAACD,WAAW,CAACE,IAAG,GAAI,EAAE;MAC1B,IAAI,CAACF,WAAW,CAACG,QAAO,GAAI,EAAE;IAChC,CAAC;IACDY,iBAAiBA,CAAA,EAAG;MAClBnB,KAAI,CACDU,GAAG,CAAC,oCAAoC,EAAE,IAAI,CAACN,WAAW,EAC1DO,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACT,OAAO,CAACU,IAAI,CAAC,GAAGD,QAAQ,CAACV,IAAI,CAAC;MACrC,CAAC,EACAY,KAAK,CAAEC,KAAK,IAAK;QAChBC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC,CAAC;IACN;EACF,CAAC;EACDK,OAAOA,CAAA,EAAG;IACR,IAAI,CAACX,UAAU,CAAC,CAAC;EACnB;AACF,CAAC"},"metadata":{},"sourceType":"module","externa